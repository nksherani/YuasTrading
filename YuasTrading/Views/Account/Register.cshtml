            @Html.CheckBoxFor(m => m.SubscribeForNewsletters, new { @checked = "checked" })
            @Html.LabelFor(m => m.SubscribeForNewsletters)
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-10">
            @Html.CheckBoxFor(m => m.AcceptTermsConditions)
            @Html.LabelFor(m => m.AcceptTermsConditions)
        </div>
    </div>
