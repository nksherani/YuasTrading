@{
    ViewBag.Title = "Register";
    //Layout = null;
}

<h2>@ViewBag.Title.</h2>
<script type="text/javascript" src="https://yuasatrading.com/Scripts/zebra_datepicker.js"></script>
<script type="text/javascript">
    $(function () {
        $('#DateOfBirth').Zebra_DatePicker({
            view: 'years',
            //format: 'M d, Y'
        });

    });
</script>
