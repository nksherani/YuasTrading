    <script>
        $('#toggle-comparelist').click(function () {
            if ($('#comparelist-panel').css('display') == 'none') {
                showCompareFlyout();
            }
            else {
                hideCompareFlyout()
            }
        });

        var compareList = new Array();


        $('.compare-add-item').click(function () {
            if (compareList.length == 4) {
                alert('Maximum 4 vehicles can be compared.');
                return false;
            }

            var i = $(this).data("sku");
            if ($.inArray(i, compareList) != -1) {
                alert('This vehicle is already in compare list.');
                return false;
            }

            $.post('/Compare/AddAjax/' + i, function (res) {
                populateCompareFlyout(res);
                compareList.push(i);
            });

            showCompareFlyout();
            $('#compare-list-count').text(parseInt($('#compare-list-count').text()) + 1);
        });

        $('.comparelist-checkbox').live('click', function () {
            if ($(this).is(':checked')) {
                compareList.push($(this).data("sku"));
            }
            else {
                compareList.splice($.inArray($(this).data("sku"), compareList), 1);
            }
        });

        $('#getQuotes-btn').click(function () {

            if ($('#comparelist-panel #CountryId').val() == "") {
                $('#comparelist-panel #CountryId').focus();
                return false;
            }

            if (compareList.length <= 1) {
                alert('Please select more than one checkbox to request for a quotation.');
                return false;
            }

            $.post('/Catalog/SubmitQuotes/?VehicleIds=' + compareList.join(",") + '&CountryId=' + $('#CountryId').val(), function () {
                hideCompareFlyout();
                $('#quoteRequestSent').modal('show');
            });
        });

        $('.compare-remove-item').live('click', function () {
            var i = $(this).data("sku");
            $.post('/Compare/RemoveAjax/' + i, function () {
                compareList.splice($.inArray(i, compareList), 1);
                $('#comparelist-panel td[data-sku="' + i + '"]').remove();
                $('#compare-list-count').text(compareList.length);

                if (compareList.length == 0) hideCompareFlyout();
            });
        });

        $('#clearCompareList-btn').on('click', function () {
            $.post('/Compare/EmptyAjax', function () {
                $('#comparelist-panel tr').each(function () {
                    $(this).find('td:gt(0)').fadeOut(function () { $(this).remove(); });
                });
                $('#compare-list-count').text('0');
                hideCompareFlyout();
                compareList.length = 0;
            });
        });

        function showCompareFlyout() {
            if ($('#compare-flyout').css('display') == 'none') {
                $('#compare-flyout').show();
            }
            $('#comparelist-panel').animate({ height: 'show' }, 300, function () { $('#toggle-comparelist .glyphicon').removeClass('glyphicon-chevron-up').addClass('glyphicon-chevron-down'); });
        }

        function hideCompareFlyout() {
            $('#comparelist-panel').animate({ height: 'hide' }, 300, function () { $('#toggle-comparelist .glyphicon').removeClass('glyphicon-chevron-down').addClass('glyphicon-chevron-up'); });
        }

        function populateCompareFlyout(p) {
            var tmpTd = '<td data-sku="' + p.Sku + '" style="width:117px;vertical-align:top !important">';
            tmpTd += '<a href="/Catalog/Details/ytc-' + p.VehicleSku + '">';
            tmpTd += '<div class="relative">';
            tmpTd += '<img class="img-thumbnail" width="96" height="60" src="/Img/Get/' + p.VehicleSku + '.jpg?w=96&h=60&sku=' + p.VehicleSku + '&isReserved=' + p.IsReserved + '&isSold=' + p.IsSold + '" title="' + p.Manufacturer + " " + p.Model + '" alt="' + p.Manufacturer + " " + p.Model + '">';
            tmpTd += '</a>';
            tmpTd += '<span class="btn btn-danger btn-xs compare-remove-item" title="Remove" data-sku="' + p.Sku + '"><span class="glyphicon glyphicon-remove"></span></span>';
            tmpTd += '</div>';
            tmpTd += '<div><a href="/Catalog/Details/ytc-' + p.VehicleSku + '">' + p.Manufacturer + ' / ' + p.Model + '</a></div>';
            tmpTd += '</td>';
            $('#comparelist-panel tr:eq(0)').append(tmpTd).fadeIn();

            $('#comparelist-panel tr:eq(1)').append('<td data-sku="' + p.Sku + '">' + p.VehicleSku + '</td>').fadeIn();
            $('#comparelist-panel tr:eq(2)').append('<td data-sku="' + p.Sku + '">' + p.ModelCode + ' | ' + p.Year + '</td>').fadeIn();
            $('#comparelist-panel tr:eq(3)').append('<td data-sku="' + p.Sku + '">' + p.Color + ' | ' + p.Transmission + '</td>').fadeIn();
            $('#comparelist-panel tr:eq(4)').append('<td data-sku="' + p.Sku + '">' + numberWithCommas(p.Mileage) + ' Km </td>').fadeIn();
            $('#comparelist-panel tr:eq(5)').append('<td data-sku="' + p.Sku + '">' + p.EnginSize + ' Lt | ' + p.FuelType + '</td>').fadeIn();
            $('#comparelist-panel tr:eq(6)').append('<td data-sku="' + p.Sku + '">' + p.Seats + '</td>').fadeIn();
            $('#comparelist-panel tr:eq(7)').append('<td data-sku="' + p.Sku + '">' + p.ShowRoom + '</td>').fadeIn();

            tmpTd = '<td data-sku="' + p.Sku + '">';
            tmpTd += p.Extras;

            if (p.IsLowMileage) {
                tmpTd += '<br />';
                tmpTd += '<span style="font-weight:bold;color:Red">Low Mileage</span>';
            }
            tmpTd += '</td>';
            $('#comparelist-panel tr:eq(8)').append(tmpTd).fadeIn();

            tmpTd = '<td data-sku="' + p.Sku + '">';
            if (p.ShowPrice) {
                tmpTd += '<strong>' + p.PriceType + ': </strong>';
                if (parseInt(p.OldPrice) > 0) {
                    tmpTd += '<span class="text-st text-muted">' + numberWithCommas(p.OldPrice) + '</span><br />';
                }
                tmpTd += '<strong>US $ ' + numberWithCommas(p.Price) + '</strong>';
            }
            tmpTd += '</td>';
            $('#comparelist-panel tr:eq(9)').append(tmpTd).fadeIn();

            $('#comparelist-panel tr:eq(10)').append('<td data-sku="' + p.Sku + '" class="text-center"><input type="checkbox" data-sku="' + p.Sku + '" class="comparelist-checkbox" checked="checked" /></td>').fadeIn();
        }

        function numberWithCommas(x) {
            return x.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
        }
    </script>





