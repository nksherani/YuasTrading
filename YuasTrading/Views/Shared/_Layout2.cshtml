    <script>
        var captchaIsValid = false;

        $(document).ready(function () {
            $('.top-menu a[href="' + window.location.pathname + '"]').addClass("active");

            $('form[data-captcha="true"]').on('submit', function (e) {
                var elm = $(this);
                if (captchaIsValid == false && elm.valid()) {
                    e.preventDefault();
                    elm.find('.captcha-validator').hide();
                    if (Recaptcha.get_response().length > 0) {
                        $.post('/Recaptcha/Validate', { challenge: Recaptcha.get_challenge(), response: Recaptcha.get_response() }, function (data) {
                            if (data) {
                                captchaIsValid = true;
                                elm.submit();
                            }
                            else {
                                Recaptcha.reload();
                                elm.find('.captcha-validator').show();
                            }
                        });
                    }
                }
            });
        });

        $.fn.resetForm = function () {
            this.find('select').each(function () { $(this).prop('selectedIndex', 0);; });
            this.find('input[type=text], input[type=password], input[type=hidden], textarea').each(function () { $(this).val(''); });
            this.find('input:checked').each(function () { $(this).removeAttr('checked'); });
        };
    </script>


